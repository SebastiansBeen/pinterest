
<style type="text/css">
  


.overdiv-index_posts {
  position: relative;
  margin-left: auto;
  margin-right: auto;
  border-right: 1px solid blue;
  border-left: 1px solid blue;
  width: 1000px;
  top: -8px;
  height: 500px;
  min-width: 1000px;
  font-weight: 300;
}

</style>

<div class='overdiv-index_posts'>

<p id="notice"><%= notice %></p>



<table>
  <thead>
    <tr>
      <th>Freelance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.description %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>

</div>
